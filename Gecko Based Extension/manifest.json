{
    "manifest_version": 2,
    "name": "Bibliography Counselor",
    "version": "1.0.0",
    "description": "A Bibliographic Citation Recommendation System integrated with Overleaf",
    "icons": {
        "16": "/icons/16_2.png",
        "32": "/icons/32_2.png",
        "64": "/icons/64_2.png",
        "128": "/icons/128_2.png"
    },
    "background": {
        "scripts" : ["background.js"]
    },
    "permissions": [
        "scripting",
        "https://overleaf.com/project/*",
        "https://www.overleaf.com/project/*",
        "*://*.overleaf.com/*"
    ],
    "browser_action": {
    "default_icon": {
                    "16": "/icons/16_2.png",
                    "32": "/icons/32_2.png"
                    },
    "default_title": "Bibliography Counselor on Overleaf"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+U"
        }
      }
    },
    "browser_specific_settings": {
     "gecko": {
        "id": "101@101.101",
        "strict_min_version": "49.0"
        }
    }


}
